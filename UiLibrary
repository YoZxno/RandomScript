--Properties:

function AstroLibrary.CreateButton(Title,Tab,Function)
	local Template = Instance.new("TextButton")
	local Color_6 = Instance.new("Frame")
	local Title_12 = Instance.new("TextLabel")
	
	Template.Name = Title
	if Tab == "Combat" then
		Template.Parent = game.CoreGui.Astro.Main.Combat.Dorpdown
	elseif Tab == "Movement" then
		Template.Parent = game.CoreGui.Astro.Main.Movement.Dorpdown
	elseif Tab == "Unility" then
		Template.Parent = game.CoreGui.Astro.Main.Unility.Dorpdown
	elseif Tab == "Visuals" then
		Template.Parent = game.CoreGui.Astro.Main.Visuals.Dorpdown
	elseif Tab == "World" then
		Template.Parent = game.CoreGui.Astro.Main.World.Dorpdown
	end
	Template.Visible = true
	Template.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	Template.BackgroundTransparency = 0.800
	Template.BorderSizePixel = 0
	Template.Size = UDim2.new(0, 153, 0, 37)
	Template.AutoButtonColor = false
	Template.Font = Enum.Font.Code
	Template.Text = " "
	Template.TextColor3 = Color3.fromRGB(255, 255, 255)
	Template.TextScaled = true
	Template.TextSize = 14.000
	Template.TextStrokeColor3 = Color3.fromRGB(255, 255, 255)
	Template.TextWrapped = true

	Color_6.Name = "Color"
	Color_6.Parent = Template
	Color_6.BackgroundColor3 = Color3.fromRGB(85, 170, 255)
	Color_6.BorderSizePixel = 0
	Color_6.Size = UDim2.new(0, 153, 0, 37)

	Title_12.Name = "Title"
	Title_12.Parent = Template
	Title_12.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Title_12.BackgroundTransparency = 1.000
	Title_12.Position = UDim2.new(0.0261437912, 0, 0.0810810775, 0)
	Title_12.Size = UDim2.new(0, 149, 0, 30)
	Title_12.Font = Enum.Font.Code
	Title_12.Text = Title
	Title_12.TextColor3 = Color3.fromRGB(255, 255, 255)
	Title_12.TextScaled = true
	Title_12.TextSize = 14.000
	Title_12.TextWrapped = true
	Title_12.TextXAlignment = Enum.TextXAlignment.Left

	-- Scripts:



	local function LNJJJOQ_fake_script() -- Template.Main 
		local script = Instance.new('LocalScript', Template)

		local Button = script.Parent
		local Color = Button.Color

		local TweanSerivce = game.TweenService

		local toggle = false

		Button.MouseEnter:Connect(function()
			if not toggle then
				Color:TweenSize(UDim2.new(0, 4,0, 37),Enum.EasingDirection.Out,Enum.EasingStyle.Sine,0.3,true)
			end
		end)

		Button.MouseLeave:Connect(function()
			if not toggle then
				Color:TweenSize(UDim2.new(0, 153,0, 37),Enum.EasingDirection.Out,Enum.EasingStyle.Sine,0.3,true)
			end
		end)




		Button.MouseButton1Click:Connect(function()
			if toggle then
				toggle = false
				Color:TweenSize(UDim2.new(0, 153,0, 37),Enum.EasingDirection.Out,Enum.EasingStyle.Sine,0.3,true)
				Function(toggle)
			else
				toggle = true
				Color:TweenSize(UDim2.new(0, 4,0, 37),Enum.EasingDirection.Out,Enum.EasingStyle.Sine,0.3,true)
				Function(toggle)
			end
		end)

	end
	coroutine.wrap(LNJJJOQ_fake_script)()
end

