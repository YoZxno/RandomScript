	local function UVZANTP_fake_script() -- Combat_2.Main 
		local script = Instance.new('LocalScript', Combat_2)

		local Button = script.Parent
		local Color = Button.Color
		local tab = script.Parent.Parent.Parent.Parent:FindFirstChild(script.Parent.Name)
		local TweanSerivce = game.TweenService

		local toggle = false

		Button.MouseEnter:Connect(function()
			if not toggle then
				Color:TweenSize(UDim2.new(0, 4,0, 37),Enum.EasingDirection.Out,Enum.EasingStyle.Sine,0.3,true)
			end
		end)

		Button.MouseLeave:Connect(function()
			if not toggle then
				Color:TweenSize(UDim2.new(0, 153,0, 37),Enum.EasingDirection.Out,Enum.EasingStyle.Sine,0.3,true)
			end
		end)




		Button.MouseButton1Click:Connect(function()
			if toggle then
				toggle = false
				Color:TweenSize(UDim2.new(0, 153,0, 37),Enum.EasingDirection.Out,Enum.EasingStyle.Sine,0.3,true)
				tab.Visible = toggle
			else
				toggle = true
				Color:TweenSize(UDim2.new(0, 4,0, 37),Enum.EasingDirection.Out,Enum.EasingStyle.Sine,0.3,true)
				tab.Visible = toggle
			end
		end)

	end
	coroutine.wrap(UVZANTP_fake_script)()
